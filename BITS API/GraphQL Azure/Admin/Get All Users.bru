meta {
  name: Get All Users
  type: http
  seq: 1
}

post {
  url: https://job-portal-bwc9haethua7h0gw.malaysiawest-01.azurewebsites.net/graphql
  body: json
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

body:json {
  {
    "query": "query GetAllUsers($role: String, $includeInactive: Boolean) { getAllUsers(role: $role, includeInactive: $includeInactive) { id name email role createdAt active bio } }",
    "variables": {
      "role": null,
      "includeInactive": false
    }
  }
}
